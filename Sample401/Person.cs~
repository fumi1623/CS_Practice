using System;
namespace Sample401 {
    class Person {
        public string name = "";
        public int age = 0;

        public void ShowAgeName() {
            Console.WriteLine("名前：{0}　年齢：{1}", name, age);
        }

        public void SetAndName(string name, int age) {
            this.name = name;
            this.age = age;
        }

    }
}
